"posts template": {
    "prefix": "post_template", 
        "body": [
            "---",
            "title:  \"[카테고리]포스트 제목(자유)\"",
            "",
            "categories:",
            "  - nav_categori_main",
            "tags:",
            "  - [tag1, tag2, tag3] ",
            "",
            "toc: true",
            "toc_sticky: true",
            "",
            "date: 2023-mm-yy",
            "last_modified_at: 2023-mm-dd",
            "---",
            "",
            "<!-- post 폴더 이름 -> 연관성을 찾지못함 ( 이상하게 바꿔도 정상적으로 작동했기때문 ) -->"
        ],
        "description": "template of posts"
    },


"categories template": {
    "prefix": "cate_tem", 
